<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.my.app.vnfr.dao.VnfrDao">

	<select id="getVnfrList" parameterType="String" resultType="VnfrVo">
		select a.id,
		       a.name,
		       a.sequence,
		       a.nsr_id,
		       a.created_at,
		       a.updated_at
		  from vnfr a
		 where a.nsr_id = #{nsrId}
	</select>

</mapper>